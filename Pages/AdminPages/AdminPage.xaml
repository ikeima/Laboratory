<Page x:Class="Laboratory.Pages.AdminPages.AdminPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Laboratory.Pages.AdminPages"
      mc:Ignorable="d" 
      d:DesignWidth="1100"
      Title="AdminPage">

    <Grid Background="White">
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Click="OpenHistoryPage" FontSize="15"  Width="250" Height="40">
                <StackPanel  Orientation="Horizontal">
                    <Image Source="/Resources/historyIcon.png"/>
                    <TextBlock Margin="5,0,0,0" TextAlignment="Left" Width="200" VerticalAlignment="Center" Text="История авторизаций"/>
                </StackPanel>
            </Button>

        </StackPanel>
    </Grid>
</Page>
